"use strict";

var path = require('path');

describe("ModuleManager", function () {
  var moduleManager = require('./../lib/module-manager.js');

  describe(".getArinModules()", function () {
    var ArinModule = require('./../lib/arin-module.js'),
        modules = moduleManager.getArinModules();

    it("should return an array", function(){
      (modules).should.be.an.Array;
    });

    if(modules.length>0){
      it("should return an array of ArinModule objects", function(){
        (modules[0]).should.be.an.instanceOf(ArinModule);
      });
    }
  });

});